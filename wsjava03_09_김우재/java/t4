class Test {
  public static void main(String args[]) {
     Super  a = new  Super ();
     Sub   b = new  Sub();
 
    if (a instanceof Super) System.out.print("1");
    if (a instanceof Sub) System.out.print("2");
    if (b instanceof Super) System.out.print("3");
    if (b instanceof Sub) System.out.print("4");   
}
}

